@model AulaRelacionamento.Models.MeusFavoritosModel

<div class="row">
    <div class="col-6">
        <ul class="list-group">
            @foreach (var item in Model.favoritos)
            {
                <li class="list-group-item">
                    @item.filme.Titulo - <a class="btn btn-danger" href="/UsariosFilmes/RemoverFavorito/@item.Id">Remover Favorito</a>
                </li>
            }
        </ul>
    </div>
    <div class="col-6">
        <ul class="list-group">
            @foreach (var item in Model.todosFilmes)
            {

                <li class="list-group-item">@item.Titulo - <a class="btn btn-success" hrer="/Usuario_Filme/Favoritar/@Model.UsuarioId/@item.Id">Favoritar</a></li>
            }
        </ul>
    </div>
</div>00000